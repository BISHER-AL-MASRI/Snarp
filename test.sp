// Define variables
var x 10
var y 20

// Add variables
add sum x y

// Print the sum
print The sum of x and y is *sum

// Define a function
Func greet() {
    print Hello, world!
)

// Call the function
greet()

// Define another function with parameters
Func addAndPrint() {
    var result 0
    add result 5 7
    print The result of adding 5 and 7 is *result
)

// Call the function with arguments
addAndPrint()

// Exit with code 0
exit 0